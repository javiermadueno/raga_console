<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="isopack.command" class="Raga\Command\ImportIsopackCommand">
            <argument type="service" id="raga.isopack.import_service" />
            <tag name="console.command" />
        </service>

        <service id="list" class="Symfony\Component\Console\Command\ListCommand">
            <tag name="console.command" />
        </service>

        <service id="help" class="Symfony\Component\Console\Command\HelpCommand">
            <tag name="console.command" />
        </service>
    </services>
</container>